{
    "name": "chubbyphp/chubbyphp-model-doctrine-dbal",
    "description": "Chubbyphp Model Doctrine Dbal",
    "keywords": ["chubbyphp", "model", "doctrine", "dbal", "slim"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dominik Zogg",
            "email": "dominik.zogg@gmail.com"
        }
    ],
    "require": {
        "php": "~7.0",
        "chubbyphp/chubbyphp-model": "~3.0",
        "container-interop/container-interop": "~1.1",
        "doctrine/common": "~2.5",
        "doctrine/dbal": "~2.5",
        "ramsey/uuid": "~3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.5",
        "pimple/pimple": "~3.0",
        "symfony/console": "~2.7|~3.0"
    },
    "suggest": {
        "chubbyphp/lazy": "~1.0",
        "chubbyphp/chubbyphp-lazy-command": "~1.0",
        "symfony/console": "~2.7|~3.0"
    },
    "provide": {
        "chubbyphp/chubbyphp-model-implementation": "~1.0@dev"
    },
    "autoload": {
        "psr-4": { "Chubbyphp\\Model\\Doctrine\\DBAL\\": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
